      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "localhost",
          "Port": 6666
        }
      ],
      "UpstreamPathTemplate": "/token",
      "UpstreamHttpMethod": [ "POST" ]
    }

  ],
  "GlobalConfiguration": {
    "BaseUrl": "http://localhost:9999"
  }
}